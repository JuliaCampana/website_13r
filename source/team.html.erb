<div class="jumbotron p-img">

</div>

<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-md-12">
        <h3>Our members</h3>
          <ul class="list-inline text-center">
            <% data.users.each do |user| %>
            <li>
              <%= image_tag "avatars/#{user.name}.png", class: "avatar" %>
              <h4><%= user.name %></h4>
              <p><%= user.function %></p>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>

